{
    "manifest_version": 3,
    "name": "Auto Tick",
    "version": "1.0",
    "description": "Automatically ticks checkboxes on any webpage.",
    "permissions": ["activeTab", "scripting", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_title": "Auto Tick"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  